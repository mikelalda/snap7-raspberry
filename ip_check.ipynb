    "# IP check"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "import sh    \n",
    "  \n",
    "  \n",
    "  \n",
    "for num in range(255):  \n",
    "    ip = \"192.168.0.\"+str(num)  \n",
    "  \n",
    "    try:  \n",
    "        sh.ping(ip, \"-c 1\",_out=\"/dev/null\")  \n",
    "        print(\"PING \",ip , \"OK\")  \n",
    "    except sh.ErrorReturnCode_1:  \n",
    "        print(\"PING \", ip, \"FAILED\")   "
   ]
  }
